import re
from urllib.request import urlopen
import pandas as pd
from io import StringIO

website_address = "http://www.cqww.com/publiclogs/2022cw/ef6t.log"
with urlopen(website_address) as response:
    html = response.read()
csv = "\n".join(re.findall(r"\nQSO:,+(.+)", re.sub(f"[ \t\r\f\v]+", ",", html.decode("unicode_escape"))))

df = pd.read_csv(StringIO(csv), header=0, names=["freq", "mode", "date", "time", "mycall", "myrst", "myzone", "call", "rst", "zone", "points"], dtype={"time": str})
df.attrs["OPERATORS"] = "EA3M"